<% include ../partials/header %>

<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1 style="text-align:center"><%= pageTitle %></h1>
    </div>
  </div>
  
  
  <div class="row">
    <div class="col-sm-12">
      <form action="/login" method="post">

        <div class="form-group">
          <input type="email" name="username" value="" placeholder="Enter email" class="form-control"/>
        </div>
        <div class="form-group">
          <input type="password" name="password" value="" placeholder="Enter password" class="form-control"/>
        </div>



        <input type="submit" name="login" value="Login" class="btn btn-success">
        <input type="reset" name="erset" value="Reset" class="btn btn-danger">
      </form>
    </div>

  </div>
</div>

